#include <iostream>
#include <string>
using namespace std;

int main(){
  int limit, a,b,c,d;
  cout << "input the limit : ";
  cin >> limit;
  a = 0;

  while (a < limit){
    a++;
    d=a % 15;
    b=a%3;
    c=a%5;
    if (d == 0){
      cout <<"  ";
      continue;
    }
    else if (c == 0){
      cout <<"& ";
      continue;
    }
    else if (b == 0){
      cout <<"# ";
      continue;
    }
    else{}

    cout <<a<<" ";
}}
